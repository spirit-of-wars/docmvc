{
  "name": "spirit-of-wars/docmvc",
  "type": "library",
  "description": "DocMVC, MVC-library for generating files (excel, doc, pdf)",
  "keywords": ["excel","doc","files","pdf", "download"],
  "license": "LGPL-2.1-or-later",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Viktor Eltyshev"
    }
  ],
  "require": {
    "php": "^7.2 || ^8.0",
    "dompdf/dompdf": "v1.0.2",
    "phpoffice/phpspreadsheet": "1.18.0",
    "phpoffice/phpword": "0.18.2",
    "psr/log": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "SpiritOfWars\\DocMVC\\": "src/DocMVC/"
    },
    "classmap": ["sample", "Test"]
  }
}
